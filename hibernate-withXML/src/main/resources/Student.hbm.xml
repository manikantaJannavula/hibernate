<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.wipro.java.hibernate.entity.Student" table="STUDENT">
	<!-- Student ID -->
	<!-- Primary key Mapping -->
		<id name="sId" type="int" column="ID">
		
         <generator class="increment"/>
      </id>
      <!-- Student Name -->
      <property name="sName" column="STUDENT_NAME" type="string" length="50" not-null="true"/>
      <!-- Student RollNO -->
      <property name="sRollNo" column="STUDENT_ROLLNO" type="string" not-null="true"></property>
      <!-- Student Class -->
      <property name="sClass" column="STUDENT_CLASS" type="string" not-null="true"></property>
	
	
	
	
	</class>
	
	
	
	
	
	



</hibernate-mapping>